{
  "auto-pull": {
    "cmds": [
      "auto-pull"
    ],
    "handler": "./src/functions/auto-pull/index.js",
    "desc": "Auto-pull from configurated repos",
    "optsData": [
      {
        "opts": [
          "-c",
          "--config"
        ],
        "desc": "configurate auto-pull behavior"
      },
      {
        "opts": [
          "--list-hidden"
        ],
        "desc": "list hidden directories on auto-pull path setting selection"
      }
    ]
  },
  "config": {
    "cmds": [
      "config"
    ],
    "handler": "./src/functions/config/index.js",
    "desc": "Configure GitHub user",
    "optsData": [
      {
        "opts": [
          "-g",
          "--global"
        ],
        "desc": "set global configuration"
      }
    ]
  },
  "push": {
    "cmds": [
      "push"
    ],
    "handler": "./src/functions/push/index.js",
    "desc": "Commit, stage and push files via interactive console",
    "optsData": []
  },
  "pull": {
    "cmds": [
      "pull"
    ],
    "handler": "./src/functions/pull/index.js",
    "desc": "Pull commits from remote repository",
    "optsData": []
  },
  "ssh": {
    "cmds": [
      "ssh"
    ],
    "handler": "./src/functions/ssh/index.js",
    "desc": "Manipulate SSH keys",
    "optsData": [
      {
        "opts": ["--generate"],
        "desc": "generate a new SSH key to use in git-assist utilities"
      },
      {
        "opts": ["--list"],
        "desc": "list all SSH keys created via git-assist"
      },
      {
        "opts": ["--delete"],
        "desc": "delete an SSH key previously generated via git-assist"
      }
    ]
  },
  "gpg": {
    "cmds": [
      "gpg"
    ],
    "handler": "./src/functions/gpg/index.js",
    "desc": "Manipulate GPG keys",
    "optsData": [
      {
        "opts": ["--generate"],
        "desc": "generate a new GPG key to use in git-assist utilities"
      },
      {
        "opts": ["--list"],
        "desc": "list all GPG keys created via git-assist"
      },
      {
        "opts": ["--delete"],
        "desc": "delete a GPG key previously generated via git-assist"
      }
    ]
  },
  "clone": {
    "cmds": [
      "clone"
    ],
    "handler": "./src/functions/clone/index.js",
    "desc": "Clone a repository",
    "optsData": []
  },
  "logs": {
    "cmds": [
      "logs"
    ],
    "handler": "./src/functions/logs/index.js",
    "desc": "Print git-assist logs",
    "optsData": [
      {
        "opts": [
          "--purge"
        ],
        "desc": "purge logs (some or all)"
      },
      {
        "opts": [
          "--watch"
        ],
        "desc": "watch a given log"
      }
    ]
  },
  "version": {
    "cmds": [
      "-v",
      "--version"
    ],
    "handler": "./src/functions/version/index.js",
    "desc": "Get current package installed version",
    "optsData": []
  },
  "help": {
    "cmds": [
      "-h",
      "--help"
    ],
    "handler": "./src/functions/help/index.js",
    "desc": "I need help!",
    "optsData": []
  }
}
